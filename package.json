{
	"name": "ebet-api",
	"version": "1.0.0",
	"description": "",
	"main": "main.ts",
	"scripts": {
		"test:static": "tsc",
		"test:unit": "jest --config=.jestrc.js --maxWorkers 2",
		"test": "npm run test:static && npm run test:unit",
		"lint": "eslint src/*.ts* src/**/*.ts*",
		"lintfix": "eslint src/*.ts* src/**/*.ts* --fix",
		"pretty-quick": "npx pretty-quick src/*.ts* src/**/*.ts*",
		"dev": "nodemon",
		"build": "npx babel src/ --out-dir .dist/ --extensions \".ts\" --source-maps inline --quiet --copy-files"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eBetApp/eBet-api.git"
	},
	"author": "",
